        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^12.5",
        "orchestra/testbench": "^10.8",
        "friendsofphp/php-cs-fixer": "^3.92"
    }
