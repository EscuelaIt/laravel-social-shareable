{
    "name": "escuelait/laravel-social-shareable",
    "version": "1.0.0",
    "description": "Create URLs to share content v√≠a social networks",
    "keywords": [
        "escuelait",
        "share",
        "social-network",
        "shareable"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/EscuelaIt/laravel-social-shareable",
    "authors": [
        {
            "name": "Miguel Angel Alvarez",
            "email": "miguel@escuela.it",
            "homepage": "https://escuela.it",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Escuelait\\SocialShareable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EscuelaIT\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Escuelait\\SocialShareable\\SocialShareableServiceProvider"
            ]
        }
